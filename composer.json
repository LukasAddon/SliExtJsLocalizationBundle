{
    "name": "sergeil/extjslocalization-bundle",
    "description": "Adds localization support for ExtJs classes to Symfony2",
    "keywords": ["extjs", "l10n", "translation"],
    "homepage": "https://github.com/sergeil/SliExtJsLocalizationBundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Sergei Lissovski",
            "email": "sergei.lissovski@gmail.com",
            "homepage": "http://sergei.lissovski.org/"
        }
    ],
    "require": {
        "php": ">=5.6",
        "symfony/translation": "^2.8|^3.4|^4.0",
        "symfony/framework-bundle": "^2.8|^3.4|^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0"
    },
    "autoload": {
        "psr-0": { "Sli\\ExtJsLocalizationBundle": "" }
    },
    "target-dir": "Sli/ExtJsLocalizationBundle"
}
